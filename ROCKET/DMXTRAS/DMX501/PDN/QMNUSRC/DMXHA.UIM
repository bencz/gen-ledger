DMXtras High Availability menu
.im #OPT

:menugrp.     HA Menus
:menui        option=1
              cond=cmxhaopr
              help='menu/option1'
              action='cmd go DMXHAOPR'
              .HA Operations menu
:menui        option=2
              cond=cmxhamon
              help='menu/option2'
              action='cmd go DMXHAMON'
              .HA Monitoring menu
:menui        option=3
              cond=cmxhacfg
              help='menu/option3'
              action='cmd go DMXHACFG'
              .HA Configuration menu
:menui        option=4
              cond=cmxhaanz
              help='menu/option4'
              action='cmd go DMXHAANZ'
              .HA Analysis menu
:menui        option=7
              cond=cmxhasos
              help='menu/option7'
              action='cmd go DMXHASOS'
              .HA Switchover menu
:menui        option=9
              cond=cmxhains
              help='menu/option9'
              action='cmd go DMXHAINS'
              .HA Installation and initial setup menu
:menui        option=50
              cond=cmxhapgm
              help='menu/option50'
              action='cmd go DMXHAPGM'
